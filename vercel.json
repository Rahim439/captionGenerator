{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/v1/predictions",
        "dest": "https://caption-generator-xi.vercel.app/api/v1/predictions"
      },
      {
        "src": "/api/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  